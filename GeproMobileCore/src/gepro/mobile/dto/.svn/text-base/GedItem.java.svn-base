package gepro.mobile.dto;

public class GedItem {	
	int ID;
	String Descricao;
	long Size;
	
	
	public long getSize() {
		return Size;
	}
	public void setSize(long size) {
		Size = size;
	}
	public int getID() {
		return ID;
	}
	public void setID(int iD) {
		ID = iD;
	}
	public String getDescricao() {
		return Descricao;
	}
	public void setDescricao(String descricao) {
		Descricao = descricao;
	}
}
