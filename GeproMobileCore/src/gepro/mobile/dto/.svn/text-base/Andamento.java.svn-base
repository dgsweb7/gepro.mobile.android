package gepro.mobile.dto;

public class Andamento {
	public int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	public int getPastaId() {
		return pastaId;
	}
	public void setPastaId(int pastaId) {
		this.pastaId = pastaId;
	}
	public int getDesdobramentoID() {
		return desdobramentoID;
	}
	public void setDesdobramentoID(int desdobramentoID) {
		this.desdobramentoID = desdobramentoID;
	}
	public Tipo getTipo() {
		return tipo;
	}
	public void setTipo(Tipo tipo) {
		this.tipo = tipo;
	}
	public String getData() {
		return Data;
	}
	public void setData(String data) {
		Data = data;
	}
	public String getDescricao() {
		return Descricao;
	}
	public void setDescricao(String descricao) {
		Descricao = descricao;
	}
	public String getResponsavel() {
		return Responsavel;
	}
	public void setResponsavel(String responsavel) {
		Responsavel = responsavel;
	}
	private int Id;
	private int pastaId;
	private int desdobramentoID;
	private Tipo tipo;
	private String Data;
	private String Descricao;
	private String Responsavel;
}
