<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:orientation="vertical"
	android:layout_height="wrap_content">


	<TextView android:text="Douglas Augusto" android:id="@+id/txt_contact_item_name"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		style="@style/txt_common.big">
	</TextView>
	<TextView android:text="Diretor Excutivo" android:id="@+id/txt_contact_item_cargo"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		style="@style/txt_common.big">
	</TextView>
	

	<TextView android:text="(00) 9999-9999" android:id="@+id/txt_contact_item_phone"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:linksClickable="true" android:autoLink="phone"
		style="@style/txt_common.medium"></TextView>

	<TextView android:text="icq@iccq.com.br" android:id="@+id/txt_contact_item_email"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:linksClickable="true" android:autoLink="email"
		style="@style/txt_common.medium"></TextView>


</LinearLayout>
